{
  "version": 2,
  "projects": {
    "builders": "libs/builders",
    "core-web-vitals-app": "apps/core-web-vitals-app",
    "core-web-vitals-app-e2e": "apps/core-web-vitals-app-e2e",
    "eslint-config": "libs/eslint-config",
    "image": "libs/image",
    "image-optimizer": "libs/image-optimizer",
    "prettier-config": "libs/prettier-config"
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "generators": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/node:library": {
      "buildable": true,
      "publishable": true
    },
    "@schematics/angular:component": {
      "flat": true,
      "skipTests": true,
      "inlineStyle": true,
      "inlineTemplate": true,
      "changeDetection": "OnPush",
      "prefix": "nge",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "nge"
    },
    "@schematics/angular:service": {
      "flat": true
    }
  },
  "defaultProject": "core-web-vitals-app"
}
