{
  "version": 1,
  "projects": {
    "builders": {
      "root": "libs/builders",
      "sourceRoot": "libs/builders/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/builders/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/builders"],
          "options": {
            "jestConfig": "libs/builders/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/builders",
            "tsConfig": "libs/builders/tsconfig.lib.json",
            "packageJson": "libs/builders/package.json",
            "main": "libs/builders/src/index.ts",
            "updateBuildableProjectDepsInPackageJson": true,
            "buildableProjectDepsInPackageJsonType": "peerDependencies",
            "assets": [
              "libs/builders/README.md",
              "libs/builders/builders.json",
              {
                "input": "libs/builders/src",
                "output": "/src",
                "glob": "**/*.json"
              }
            ]
          }
        },
        "command": {
          "builder": "./dist/libs/builders:command",
          "options": {
            "command": "cmd",
            "args": ["/c", "dir"]
          }
        },
        "release": {
          "builder": "./dist/libs/builders:semantic-release",
          "configurations": {
            "local": {
              "force": true
            }
          }
        }
      }
    },
    "core-web-vitals": {
      "projectType": "application",
      "root": "apps/core-web-vitals",
      "sourceRoot": "apps/core-web-vitals/src",
      "prefix": "ng-easy",
      "architect": {
        "browser": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/core-web-vitals/browser",
            "index": "apps/core-web-vitals/src/index.html",
            "main": "apps/core-web-vitals/src/main.ts",
            "polyfills": "apps/core-web-vitals/src/polyfills.ts",
            "tsConfig": "apps/core-web-vitals/tsconfig.app.json",
            "assets": ["apps/core-web-vitals/src/favicon.ico", "apps/core-web-vitals/src/assets"],
            "styles": ["apps/core-web-vitals/src/styles.scss"],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/core-web-vitals/src/environments/environment.ts",
                  "with": "apps/core-web-vitals/src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                "scripts": true,
                "fonts": {
                  "inline": true
                },
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {}
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "sourceMap": {
              "scripts": true,
              "styles": true,
              "vendor": true
            }
          },
          "configurations": {
            "production": {
              "browserTarget": "core-web-vitals:browser:production"
            },
            "development": {
              "browserTarget": "core-web-vitals:browser:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "core-web-vitals:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/core-web-vitals/src/**/*.ts", "apps/core-web-vitals/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/core-web-vitals"],
          "options": {
            "jestConfig": "apps/core-web-vitals/jest.config.js",
            "passWithNoTests": true,
            "aot": true
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "outputs": ["dist/apps/core-web-vitals/server"],
          "options": {
            "outputPath": "dist/apps/core-web-vitals/server",
            "main": "apps/core-web-vitals/src/main.server.ts",
            "tsConfig": "apps/core-web-vitals/tsconfig.server.json",
            "sourceMap": true,
            "optimization": false
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/core-web-vitals/src/environments/environment.ts",
                  "with": "apps/core-web-vitals/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            },
            "development": {}
          },
          "defaultConfiguration": "production"
        },
        "build": {
          "builder": "@angular-devkit/build-angular:app-shell",
          "outputs": ["dist/apps/core-web-vitals"],
          "options": {
            "route": "shell"
          },
          "configurations": {
            "production": {
              "browserTarget": "core-web-vitals:browser:production",
              "serverTarget": "core-web-vitals:server:production"
            },
            "development": {
              "serverTarget": "core-web-vitals:server:development",
              "browserTarget": "core-web-vitals:browser:development"
            }
          },
          "defaultConfiguration": "production"
        },
        "favicons": {
          "builder": "./dist/libs/builders:favicons",
          "outputs": ["dist/apps/core-web-vitals/browser"],
          "options": {
            "browserTarget": "core-web-vitals:browser",
            "favicon": "apps/core-web-vitals/src/favicon.svg",
            "background": "#303030"
          },
          "configurations": {
            "production": {
              "browserTarget": "core-web-vitals:browser:production",
              "background": "#1353ab"
            }
          }
        }
      }
    },
    "core-web-vitals-e2e": {
      "root": "apps/core-web-vitals-e2e",
      "sourceRoot": "apps/core-web-vitals-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/core-web-vitals-e2e/cypress.json",
            "tsConfig": "apps/core-web-vitals-e2e/tsconfig.e2e.json",
            "devServerTarget": "core-web-vitals:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "core-web-vitals:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/core-web-vitals-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "eslint-config": {
      "root": "libs/eslint-config",
      "sourceRoot": "libs/eslint-config/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/eslint-config/**/*.js"]
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/eslint-config",
            "tsConfig": "libs/eslint-config/tsconfig.lib.json",
            "packageJson": "libs/eslint-config/package.json",
            "main": "libs/eslint-config/index.js",
            "assets": ["libs/eslint-config/README.md"]
          }
        },
        "release": {
          "builder": "./dist/libs/builders:semantic-release"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/node:library": {
      "buildable": true,
      "publishable": true
    },
    "@schematics/angular:component": {
      "flat": true,
      "skipTests": true,
      "inlineStyle": true,
      "inlineTemplate": true,
      "changeDetection": "OnPush",
      "prefix": "nge",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "nge"
    },
    "@schematics/angular:service": {
      "flat": true
    }
  },
  "defaultProject": "core-web-vitals"
}
